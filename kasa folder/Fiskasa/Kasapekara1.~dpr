program Kasapekara1;

uses
  Forms,
  Kasapekara in 'Kasapekara.pas' {Form1},
  Unit2 in 'Unit2.pas' {Formakase},
  Unit3 in 'Unit3.pas' {Formablagajne},
  Unit4 in 'Unit4.pas' {Artikli},
  Login in 'Login.pas' {Logiranje},
  fiska in 'fiska.pas' {Glavna},
  UnitKasa in 'UnitKasa.pas' {frmBlagajna},
  UnitRepro in 'UnitRepro.pas' {FRepro},
  untMain in 'untMain.pas' {frmMain},
  Unitzakljuci in 'Unitzakljuci.pas' {Formazakljuci},
  Total in 'Total.pas' {TotalForm},
  UnitParagon in 'UnitParagon.pas' {FormParagon},
  EnterR1 in 'EnterR1.pas' {EnterR1Form};

{$R *.res}

begin
  Application.Initialize;
  Application.Title := 'Kasa 1.1.1010';
  Application.CreateForm(TLogiranje, Logiranje);
  Application.CreateForm(TFormakase, Formakase);
  Application.CreateForm(TFRepro, FRepro);
  Application.CreateForm(TForm1, Form1);
  Application.CreateForm(TFormablagajne, Formablagajne);
  Application.CreateForm(TArtikli, Artikli);
  Application.CreateForm(TGlavna, Glavna);
  Application.CreateForm(TfrmBlagajna, frmBlagajna);
  Application.CreateForm(TfrmMain, frmMain);
  Application.CreateForm(TFormazakljuci, Formazakljuci);
  Application.CreateForm(TTotalForm, TotalForm);
  Application.CreateForm(TFormParagon, FormParagon);
  Application.CreateForm(TEnterR1Form, EnterR1Form);
  Application.Run;
end.
